{
  "version": 2,
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/login.php",
      "dest": "/login.php"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/constant/(.*)",
      "dest": "/constant/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(css|js|jpg|jpeg|png|gif|svg|ico|pdf|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/login.php"
    }
  ],
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
